<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Séquence pour les réceptions -->
        <record id="seq_e_gestock_reception" model="ir.sequence">
            <field name="name">Séquence pour les réceptions</field>
            <field name="code">e_gestock.reception</field>
            <field name="prefix">RCP/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

